{
  "name": "vite-plugin-lit-css",
  "version": "0.0.5",
  "homepage": "https://github.com/redfox-mx/vite-lit-css/blob/main/packages/vite-plugin-lit-css/README.md",
  "repository": "https://github.com/redfox-mx/vite-lit-css",
  "author": {
    "name": "Diego Jesus Hernandez Gonzalez",
    "email": "hernandezdj98@outlook.com",
    "aka": "redfox-mx"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
