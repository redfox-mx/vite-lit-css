  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild && pnpm run patch-cjs",
    "patch-cjs": "tsx ../../scripts/patchCJS.ts",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vite": "^4.2.0",
    "lit": "^2.0.0"
  },
  "devDependencies": {
    "es-module-lexer": "^1.2.1",
    "magic-string": "^0.30.0"
  }
}